{
  "presets": [
    ["env", { "modules": false }],
    "stage-2",
    "es2015"
  ],
  "plugins": ["transform-runtime",["component",[
    {
      "libraryName": "iview",
      "styleLibraryName": "theme-default"
    }
  ]]],
  "comments": false,
  "env": {
    "test": {
      "presets": ["env", "stage-2"],
      "plugins": [ "istanbul" ]
    }
  }
}
